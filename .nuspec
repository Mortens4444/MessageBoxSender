    <file src=".\MessageBoxSender\bin\Release\net8.0-windows\**" target="lib\net8.0-windows7" exclude=".\MessageBoxSender\bin\Release\net8.0-windows\MessageBoxSender.*" />
    <file src=".\MessageBoxSender\bin\Release\net9.0-windows\**" target="lib\net9.0-windows7" exclude=".\MessageBoxSender\bin\Release\net9.0-windows\MessageBoxSender.*" />
	<file src=".\ReadMe.md" target="docs\" />
