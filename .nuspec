	<projectUrl>https://github.com/Mortens4444/MessageBoxSender</projectUrl>
    <repository type="git" url="https://github.com/Mortens4444/MessageBoxSender.git" branch="main" commit="1f98aaeeac94c425257aedcb5a2d93a8d69a26fe" />
	<tags>Automatic AutoClose MessageBoxes ConfirmBox DebugErrorBox ErrorBox InfoBox Windows Forms</tags>
	<readme>docs\ReadMe.md</readme>
    <dependencies>
      <group targetFramework="net48" />
      <group targetFramework="net8.0-windows10.0" />
      <group targetFramework="net9.0-windows10.0" />
    </dependencies>
  </metadata>
