	<tags>MessageBoxes ConfirmBox DebugErrorBox ErrorBox InfoBox Windows Forms</tags>
	<readme>docs\ReadMe.md</readme>
    <dependencies>
      <group targetFramework="net48" />
      <group targetFramework="net8.0-windows10.0" />
    </dependencies>
  </metadata>
  <files>
    <file src=".\MessageBoxSender\bin\Release\net481\**" target="lib\net48" exclude=".\MessageBoxSender\bin\Release\net481\MessageBoxSender.exe" />
