<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Mtf.MessageBoxes</id>
    <version>1.0.13</version>
    <description>
      Custom Message Boxes for enhanced user interaction.
      
      The library includes various message boxes, such as:
      - ConfirmBox
      - ErrorBox
      - InfoBox
      - DebugErrorBox
      
      Features:
      - Copyable Error Messages: Easily copy error messages directly from the ErrorBox, streamlining the reporting process.
      - Configurable Timeout: Set a timeout for the message boxes, allowing them to close automatically after a specified duration, improving user experience.
      
      Compatibility:
      - .NET Framework 4.8.1
      - .NET 8
      - .NET 9
	</description>
    <authors>Mortens</authors>
	<owners>Mortens</owners>
    <copyright>Copyright © 2024</copyright>
	<license type="expression">MIT</license>
	<projectUrl>https://github.com/Mortens4444/MessageBoxSender</projectUrl>
    <repository type="git" url="https://github.com/Mortens4444/MessageBoxSender.git" branch="main" commit="8f0b2c1841776e8eee9777d585874eeac62fe58b" />
	<tags>Automatic AutoClose MessageBoxes ConfirmBox DebugErrorBox ErrorBox InfoBox Windows Forms Mortens</tags>
	<readme>docs\ReadMe.md</readme>
    <dependencies>
	  <group targetFramework="net481">
		<dependency id="System.Management" version="4.7.0" />
	  </group>
	  <group targetFramework="net8.0-windows7">
		<dependency id="System.Management" version="8.0.0" />
	  </group>
	  <group targetFramework="net9.0-windows7">
		<dependency id="System.Management" version="9.0.0" />
	  </group>
    </dependencies>
  </metadata>
  <files>
